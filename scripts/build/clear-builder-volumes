#!/usr/bin/env bash

set -euo pipefail

# Clear all the volumes that we use

# Must be run from outside (stopped) container

docker volume rm \
  dark_stable_build \
  dark_stable_dotesy \
  dark_stable_esy \
  dark_stable_fsharp_build \
  dark_stable_lib \
  dark_stable_node_modules \
  dark_stable_nuget \
  darklang-dark-stable-extension-volume \
  darklang-dark-stable-extension-volume-insiders \
  vscode-dark-stable